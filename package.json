{
  "name": "js-magic",
  "version": "1.2.4",
  "description": "JavaScript magic methods support.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc --module commonjs --target es2015 --sourceMap true --declaration true index.ts",
    "prepublishOnly": "npm run build",
    "postpublish": "ls index.* | grep -v index.ts | xargs rm -f",
    "pretest": "tsc --module commonjs --target es2015 test.ts",
    "posttest": "rm -f test.js* && ls index.js* | xargs rm -f",
    "test": "mocha"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayonli/js-magic.git"
  },
  "keywords": [
    "Magic Methods",
    "getter",
    "setter"
  ],
  "author": "A-yon Lee <the@ayon.li>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ayonli/js-magic/issues"
  },
  "homepage": "https://github.com/ayonli/js-magic#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.14.31",
    "mocha": "^5.2.0",
    "typescript": "^4.9.5"
  }
}
